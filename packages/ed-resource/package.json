{
  "name": "@moodlenet/ed-resource",
  "version": "0.7.3",
  "publishConfig": {
    "access": "public"
  },
  "author": "MoodleNet",
  "moodlenet": {
    "displayName": "Resource page",
    "icon": "https://moodle.net/moodlenet-logo-small.svg"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/moodlenet/moodlenet"
  },
  "keywords": [
    "moodlenetPackage"
  ],
  "description": "Page to display resource information",
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": "16",
    "npm": "8",
    "yarn": "please-use-npm"
  },
  "readme": "",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@moodlenet/component-library": "0.7.3",
    "@moodlenet/core-domain": "0.7.3",
    "@moodlenet/key-value-store": "0.7.3",
    "@moodlenet/simple-file-store": "0.7.3",
    "formik": "2.2.9",
    "moment": "2.29.4",
    "xbytes": "1.8.0",
    "xstate": "4.38.3",
    "yup": "0.32.11"
  },
  "peerDependencies": {
    "@mui/icons-material": "5.14.9",
    "@mui/material": "5.14.9",
    "@moodlenet/core": "0.7.3",
    "@moodlenet/ed-meta": "0.7.3",
    "@moodlenet/http-server": "0.7.3",
    "@moodlenet/react-app": "0.7.3",
    "@moodlenet/system-entities": "0.7.3"
  },
  "devDependencies": {
    "@lingui/macro": "3.14.0",
    "@moodlenet/core": "0.7.3",
    "@moodlenet/ed-meta": "0.7.3",
    "@moodlenet/http-server": "0.7.3",
    "@moodlenet/react-app": "0.7.3",
    "@moodlenet/system-entities": "0.7.3",
    "@storybook/addon-actions": "7.4.2",
    "@storybook/react": "7.4.2",
    "@types/node": "17.0.35",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "react-router-dom": "6.4.1",
    "typed-emitter": "2.1.0",
    "typescript": "4.7.2",
    "@mui/icons-material": "5.14.9",
    "@mui/material": "5.14.9"
  },
  "scripts": {
    "dev-assistant": "npm-watch copy-assets & npm-watch create-components-index & npm-watch create-stories-index",
    "prepare": "npm run copy-assets",
    "copy-assets": "copyfiles -u 1 src/**/*.{scss,png,svg,json,jpg} dist",
    "create-components-index": "vgis src/webapp/exports/ui.mts",
    "create-stories-index": "vgis src/webapp/exports/stories.mts"
  },
  "watch": {
    "create-components-index": {
      "patterns": [
        "src/webapp/**/*"
      ],
      "extensions": ".ts,.tsx,.mts",
      "delay": 300
    },
    "create-stories-index": {
      "patterns": [
        "src/webapp/**/*"
      ],
      "extensions": ".ts,.tsx,.mts",
      "delay": 300
    },
    "copy-assets": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": ".scss,.png,.svg,.json",
      "ignore": "**/*.*ts*",
      "delay": 300
    }
  },
  "exports": {
    "./init": "./dist/server/init.mjs",
    "./server": "./dist/server/exports.mjs",
    "./common": "./dist/common/exports.mjs",
    "./ui": "./dist/webapp/exports/ui.mjs",
    "./stories": "./dist/webapp/exports/stories.mjs",
    "./webapp": "./dist/webapp/exports/webapp.mjs",
    "./@moodlenet/react-app/init": "./dist/webapp/exports/init.mjs"
  },
  "gitHead": "4b8737c02083f2eca71b432c601ba3f595cd4f51"
}
