    "moodlenetPackage"
  ],
  "description": "HTTP server\nMain system HTTP server",
  "files": [
    "dist/",
    "src/"
  ],
  "repository": "https://gitlab.com/moodlenet/moodlenet",
  "author": "https://github.com/aleclofabbro",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "express": "^4.18.1",
    "http-graceful-shutdown": "^3.1.7"
  },
  "peerDependencies": {
    "@moodlenet/authentication-manager": "^0.2.0",
    "@moodlenet/core": "^0.2.0"
  },
  "devDependencies": {
    "@moodlenet/authentication-manager": "^0.2.0",
    "@moodlenet/core": "^0.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/node": "^17.0.35",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "tsc-build": "tsc --build",
    "tsc-complete": "npm run tsc-move-output",
    "cleanup-all": "npm run cleanup-build && rimraf node_modules && rimraf package-lock-json && rimraf yarn-lock",
    "cleanup-build": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "tsc-move-output": "copyfiles -u 1 src/**/*.{mjs,mjs.map,d.mts,d.mts.map,js,js.map,d.ts,d.ts.map} dist && rimraf src/**/*.{mjs,mjs.map,d.mts,d.mts.map,js,js.map,d.ts,d.ts.map}"
