  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": "./dist/main.mjs",
    "./lib": "./dist/ext-ports-app/pub-lib.mjs"
  },
  "moodlenet": {
    "creator": "MoodleNet",
    "displayName": "HTTP server"
  },
