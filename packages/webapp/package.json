{
  "name": "my-moodlenet-webapp",
  "version": "0.0.1-beta.0.9",
  "license": "AGPL",
  "description": "MoodleNet Webapp",
  "author": "https://github.com/aleclofabbro",
  "files": [
    "build/",
    "publicFolder.js"
  ],
  "dependencies": {
    "@apollo/client": "^3.4.11",
    "@lingui/macro": "^3.5.0",
    "@lingui/react": "^3.5.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link-context": "^1.0.20",
    "apollo-link-logger": "^2.0.0",
    "formik": "^2.2.6",
    "graphql": "^15.5.0",
    "moment": "^2.29.1",
    "my-moodlenet-common": "^0.0.1-beta.0.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-use-what-input": "^1.2.0",
    "web-vitals": "^0.2.4",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.3",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/schema-ast": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.18.1",
    "@storybook/addon-actions": "^6.3.2",
    "@storybook/addon-essentials": "^6.3.2",
    "@storybook/addon-links": "^6.3.2",
    "@storybook/node-logger": "^6.3.2",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.3.2",
    "@storybook/theming": "^6.3.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lingui__macro": "^2.7.4",
    "@types/lingui__react": "^2.8.3",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/yup": "^0.29.11",
    "babel-loader": "8.1.0",
    "cross-env": "^7.0.3",
    "graphql-cli": "^4.1.0",
    "react-scripts": "4.0.1",
    "serve": "^11.3.2",
    "storybook": "^6.3.2",
    "storybook-addon-linguijs": "^1.0.10",
    "svg-url-loader": "^7.1.1",
    "tuple-type": "^1.0.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "gen:graphql:types": "graphql-codegen --config ./graphql.codegen.config.js",
    "start-dev": "cross-env NODE_ENV=development react-scripts start --incremental",
    "start": "cross-env NODE_ENV=production serve -s -l 3000 build",
    "build": "yarn gen:graphql:types ; react-scripts build",
    "clean-build": "rm -rf build && yarn build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepublish": "yarn clean-build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:react-hooks/recommended"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error"
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
