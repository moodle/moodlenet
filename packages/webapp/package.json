{
  "name": "@moodlenet/webapp",
  "version": "0.0.1",
  "description": "",
  "moodlenet": {
    "name": "moodlenet webapp",
    "shortDesc": "a desc for moodlenet webapp"
  },
  "main": "lib/index.js",
  "node_engine": {
    "node": ">=10.16.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "react-hot-loader/babel"
      ],
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@moodlenet/kernel": "^0.0.1",
    "@moodlenet/pri-http": "^0.0.1",
    "@svgr/webpack": "^5.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.179",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "compression-webpack-plugin": "^4.0.1",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^3.6.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^4.3.8",
    "less": "^3.13.1",
    "less-loader": "^6.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^2.0.2",
    "serve": "^11.3.2",
    "shx": "^0.3.4",
    "style-loader": "^1.3.0",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.3",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "@ant-design/dark-theme": "^2.0.2",
    "@ant-design/icons": "^4.7.0",
    "@hot-loader/react-dom": "^17.0.2",
    "antd": "^4.19.1",
    "beautify-json": "^1.0.1",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-router-dom": "^6.2.2"
  }
}
