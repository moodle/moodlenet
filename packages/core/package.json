{
  "name": "@moodlenet/core",
  "version": "0.2.0",
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "moodlenet": {
    "creator": "MoodleNet",
    "displayName": "Core"
  },
  "keywords": [
    "moodlenetPackage"
  ],
  "description": "Core\nThe kernel of MoodleNet",
  "files": [
    "dist/",
    "src/",
    "bin/"
  ],
  "bin": "./bin/boot.mjs",
  "exports": {
    ".": "./dist/main.mjs"
  },
  "repository": "https://gitlab.com/moodlenet/moodlenet",
  "author": "https://github.com/aleclofabbro",
  "dependencies": {
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "execa": "^5.1.1",
    "npm-check-updates": "^16.3.18",
    "pkg-dir": "^7.0.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.6",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/semver": "^7.3.9",
    "@types/yargs": "^17.0.10",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "tsc-build": "tsc --build",
    "tsc-complete": "npm run tsc-move-output",
    "cleanup-all": "npm run cleanup-build && rimraf node_modules && rimraf package-lock-json && rimraf yarn-lock",
    "cleanup-build": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "tsc-move-output": "copyfiles -u 1 src/**/*.{mjs,mjs.map,d.mts,d.mts.map,js,js.map,d.ts,d.ts.map} dist && rimraf src/**/*.{mjs,mjs.map,d.mts,d.mts.map,js,js.map,d.ts,d.ts.map}"
  }
}
