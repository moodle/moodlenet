{
  "extends": "../../tsconfig.json",
  // "include": ["src/ui", "src/custom.d.ts"],
  "exclude": ["lib/**/*"],
  "compilerOptions": {
    "rootDirs": ["src", "react-app-lib"],
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ES5",
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "jsx": "react-jsx"
  }
}
