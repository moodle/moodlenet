{
  "extends": "../../tsconfig.json",
  // "include": ["src/ui", "src/custom.d.ts"],
  "exclude": ["lib/**/*", "build", "latest-build"],
  "compilerOptions": {
    "sourceMap": true,
    "rootDir": "src",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ES5",
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "jsx": "react-jsx",
    "allowJs": true
  }
}
