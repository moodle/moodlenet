{
  "extends": "../../tsconfig.json",
  "exclude": ["lib/**/*"],
  "compilerOptions": {
    "rootDir": "src",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ES5",
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "jsx": "react-jsx"
  }
}
