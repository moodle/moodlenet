{
  "name": "@moodlenet/authentication-manager",
  "version": "0.2.0",
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": "./dist/main.mjs"
  },
  "moodlenet": {
    "creator": "MoodleNet",
    "displayName": "Authentication manager"
  },
  "keywords": [
    "moodlenetPackage"
  ],
  "description": "Authentication manager\nManager for multiple authentication systems",
  "files": [
    "dist/",
    "src/"
  ],
  "repository": "https://gitlab.com/moodlenet/moodlenet",
  "author": "https://github.com/aleclofabbro",
  "optDependencies": {},
  "devDependencies": {
    "@moodlenet/arangodb": "^0.2.0",
    "@moodlenet/core": "^0.2.0",
    "@moodlenet/crypto": "^0.2.0",
    "@types/node": "^17.0.35",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "dev-assistant": "npm-watch copy-assets",
    "copy-assets": "copyfiles -u 1 src/**/*.{scss,png,svg,json} dist",
    "tsc-build": "tsc --build",
    "tsc-complete": "npm run tsc-move-output",
    "cleanup-all": "npm run cleanup-build && rimraf node_modules && rimraf package-lock-json && rimraf yarn-lock",
    "cleanup-build": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "tsc-move-output": "copyfiles -u 1 src/**/*.{mjs,mjs.map,d.mts,d.mts.map,js,js.map,d.ts,d.ts.map} dist && rimraf src/**/*.{mjs,mjs.map,d.mts,d.mts.map,js,js.map,d.ts,d.ts.map}"
  },
  "watch": {
    "copy-assets": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": ".scss,.png,.svg,.json",
      "ignore": "**/*.*ts*",
      "delay": 300
    }
  }
}
