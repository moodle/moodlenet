{
  "name": "@moodlenet/component-library",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "main": "lib/main.mjs",
  "readme": "",
  "files": [
    "lib/",
    "src/",
    "public/",
    "public/favicon*",
    "_redirects",
    "moodlenet-component-library-lib.d.ts"
  ],
  "moodlenet": {
    "creator": "MoodleNet",
    "displayName": "Web application"
  },
  "keywords": [
    "moodlenetPackage"
  ],
  "description": "Web application\nFrontend interface to interact with the backend",
  "exports": {
    ".": "./lib/main.mjs"
  },
  "scripts": {
    "dev-assistant": "npm-watch copy-assets & npm-watch create-indexes",
    "copy-assets": "copyfiles -u 1 src/**/*.{scss,png,svg,json} lib",
    "create-indexes": "vgis src/main.mts"
  },
  "watch": {
    "create-indexes": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": ".ts,.tsx,.mts",
      "delay": 300
    },
    "copy-assets": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": ".scss,.png,.svg,.json",
      "ignore": "**/*.*ts*",
      "delay": 300
    }
  },
  "author": "",
  "peerDependencies": {},
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3"
  }
}
