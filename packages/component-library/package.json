{
  "name": "@moodlenet/component-library",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "readme": "",
  "files": [
    "lib/",
    "src/",
    "public/",
    "public/favicon*",
    "_redirects",
    "moodlenet-component-library-lib.d.ts"
  ],
  "moodlenet": {
    "creator": "MoodleNet",
    "displayName": "Web application"
  },
  "keywords": [
    "moodlenetPackage"
  ],
  "description": "Web application\nFrontend interface to interact with the backend",
  "main": "./lib/components.mjs",
  "exports": {
    ".": "./lib/components.mjs"
  },
  "scripts": {
    "dev-assistant": "npm-watch copy-assets & npm-watch create-components-index & npm-watch create-stories-index",
    "copy-assets": "copyfiles -u 1 src/**/*.{scss,png,svg,json} lib",
    "create-components-index": "vgis src/components.mts",
    "create-stories-index": "vgis src/stories.mts"
  },
  "watch": {
    "create-stories-index": {
      "patterns": [
        "src/stories.mts",
        "src/**/*.stories.tsx"
      ],
      "extensions": ".ts,.tsx,.mts",
      "delay": 300
    },
    "create-components-index": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": ".ts,.tsx,.mts",
      "delay": 300
    },
    "copy-assets": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": ".scss,.png,.svg,.json",
      "ignore": "**/*.*ts*",
      "delay": 300
    }
  },
  "author": "",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.2",
    "@material-ui/icons": "^4.11.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3"
  }
}
