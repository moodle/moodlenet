{
  "name": "@moodlenet/extensions-manager",
  "version": "0.1.0",
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "main": "lib/main.mjs",
  "moodlenet": {
    "creator": "MoodleNet",
    "displayName": "Extensions manager"
  },
  "keywords": [
    "moodlenetPackage"
  ],
  "description": "Extensions manager\nManager for system extensions",
  "files": [
    "lib/",
    "src/"
  ],
  "repository": "https://gitlab.com/moodlenet/moodlenet",
  "author": "https://github.com/aleclofabbro",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.2",
    "axios": "^0.27.2",
    "react-markdown": "^8.0.3",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^6.1.1"
  },
  "peerDependencies": {
    "@moodlenet/core": "^0.1.0",
    "@moodlenet/react-app": "^0.1.0",
    "@moodlenet/component-library": "^0.1.0"
  },
  "devDependencies": {
    "@moodlenet/core": "^0.1.0",
    "@moodlenet/react-app": "^0.1.0",
    "@moodlenet/component-library": "^0.1.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-syntax-highlighter": "^15.5.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "dev-assistant": "npm-watch copy-assets",
    "copy-assets": "copyfiles -u 1 src/**/*.{scss,png,svg,json} lib"
  },
  "watch": {
    "copy-assets": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": ".scss,.png,.svg,.json",
      "ignore": "**/*.*ts*",
      "delay": 300
    }
  }
}
