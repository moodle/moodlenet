{
  "name": "@moodlenet/web-user",
  "version": "0.1.0",
  "author": "MoodleNet",
  "moodlenet": {
    "displayName": "Profile page",
    "icon": "https://moodle.net/moodlenet-logo-small.svg"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/moodlenet/moodlenet/-/tree/moodlenet3-dev/packages/component-library"
  },
  "keywords": [
    "moodlenetPackage"
  ],
  "description": "Page to display user public information",
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "main": "lib/main.mjs",
  "readme": "",
  "files": [
    "lib/"
  ],
  "peerDependencies": {
    "@moodlenet/authentication-manager": "^0.1.0",
    "@moodlenet/component-library": "^0.1.0",
    "@moodlenet/content-graph": "^0.1.0",
    "@moodlenet/core": "^0.1.0",
    "@moodlenet/react-app": "^0.1.0"
  },
  "devDependencies": {
    "@lingui/macro": "^3.14.0",
    "@moodlenet/authentication-manager": "^0.1.0",
    "@moodlenet/component-library": "^0.1.0",
    "@moodlenet/content-graph": "^0.1.0",
    "@moodlenet/core": "^0.1.0",
    "@moodlenet/react-app": "^0.1.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.7.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "dev-assistant": "npm-watch copy-assets & npm-watch create-components-index",
    "copy-assets": "copyfiles -u 1 src/**/*.{scss,png,svg,json} lib",
    "create-components-index": "vgis src/ui.mts"
  },
  "watch": {
    "create-components-index": {
      "patterns": [
        "src/webapp/**/*"
      ],
      "extensions": ".ts,.tsx,.mts",
      "delay": 300
    },
    "copy-assets": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": ".scss,.png,.svg,.json",
      "ignore": "**/*.*ts*",
      "delay": 300
    }
  },
  "exports": {
    ".": "./lib/main.mjs",
    "./ui": "./lib/ui.mjs",
    "./stories": "./lib/stories.mjs"
  }
}
