{
  "name": "@moodlenet/simple-email-auth",
  "version": "0.2.0",
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "moodlenet": {
    "creator": "MoodleNet",
    "displayName": "Email authentication"
  },
  "keywords": [
    "moodlenetPackage"
  ],
  "description": "Email authentication\nBasic authentication using email and password",
  "files": [
    "dist/",
    "src/"
  ],
  "exports": {
    ".": "./dist/main.mjs",
    "./ui": "./dist/webapp/ui.mjs"
  },
  "repository": "https://gitlab.com/moodlenet/moodlenet",
  "author": "https://github.com/aleclofabbro",
  "dependencies": {
    "formik": "^2.2.9",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "@moodlenet/arangodb": "^0.2.0",
    "@moodlenet/authentication-manager": "^0.2.0",
    "@moodlenet/component-library": "^0.2.0",
    "@moodlenet/content-graph": "^0.2.0",
    "@moodlenet/core": "^0.2.0",
    "@moodlenet/crypto": "^0.2.0",
    "@moodlenet/email-service": "^0.2.0",
    "@moodlenet/http-server": "^0.2.0",
    "@moodlenet/react-app": "^0.2.0",
    "@moodlenet/web-user": "^0.2.0"
  },
  "devDependencies": {
    "@moodlenet/arangodb": "^0.2.0",
    "@moodlenet/authentication-manager": "^0.2.0",
    "@moodlenet/component-library": "^0.2.0",
    "@moodlenet/content-graph": "^0.2.0",
    "@moodlenet/core": "^0.2.0",
    "@moodlenet/crypto": "^0.2.0",
    "@moodlenet/email-service": "^0.2.0",
    "@moodlenet/http-server": "^0.2.0",
    "@moodlenet/react-app": "^0.2.0",
    "@moodlenet/web-user": "^0.2.0",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "dev-assistant": "npm-watch copy-assets & npm-watch create-components-index",
    "copy-assets": "copyfiles -u 1 src/**/*.{scss,png,svg,json} dist",
    "create-components-index": "vgis src/webapp/ui.mts",
    "tsc-build": "tsc --build",
    "tsc-complete": "npm run tsc-move-output",
    "cleanup-all": "npm run cleanup-build && rimraf node_modules && rimraf package-lock-json && rimraf yarn-lock",
    "cleanup-build": "rimraf dist && rimraf tsconfig.tsbuildinfo",
    "tsc-move-output": "copyfiles -u 1 src/**/*.{mjs,mjs.map,d.mts,d.mts.map,js,js.map,d.ts,d.ts.map} dist && rimraf src/**/*.{mjs,mjs.map,d.mts,d.mts.map,js,js.map,d.ts,d.ts.map}"
  },
  "watch": {
    "create-components-index": {
      "patterns": [
        "src/webapp/**/*"
      ],
      "extensions": ".ts,.tsx,.mts",
      "delay": 300
    },
    "copy-assets": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": ".scss,.png,.svg,.json",
      "ignore": "**/*.*ts*",
      "delay": 300
    }
  }
}
