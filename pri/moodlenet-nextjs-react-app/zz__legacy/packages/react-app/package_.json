{
  "name": "@moodlenet/react-app",
  "version": "0.9.1",
  "publishConfig": {
    "access": "public"
  },
  "author": "MoodleNet",
  "description": "Frontend interface to interact with the backend",
  "moodlenet": {
    "displayName": "Web application",
    "icon": "https://moodle.net/moodlenet-logo-small.svg"
  },
  "repository": "https://github.com/moodle/moodlenet",
  "license": "AGPL-3.0",
  "type": "module",
  "engines": {
    "node": "20",
    "npm": "10",
    "yarn": "please-use-npm"
  },
  "readme": "",
  "files": [
    "dist/",
    "src/",
    "public/",
    "public/favicon*",
    "_redirects",
    "moodlenet-react-app-lib.d.ts"
  ],
  "keywords": ["moodlenetPackage"],
  "exports": {
    "./init": "./dist/server/init.mjs",
    "./start": "./dist/server/start.mjs",
    "./server": "./dist/server/exports.mjs",
    "./common": "./dist/common/exports.mjs",
    "./ui": "./dist/webapp/exports/ui.mjs",
    "./stories": "./dist/webapp/exports/stories.mjs",
    "./webapp": "./dist/webapp/exports/webapp.mjs",
    "./@moodlenet/react-app/init": "./dist/webapp/exports/init.mjs"
  },
  "scripts": {
    "dev-assistant": "npm-watch copy-assets & npm-watch create-components-index & npm-watch create-stories-index",
    "prepare": "npm run copy-assets",
    "copy-assets": "copyfiles -u 1 src/**/*.{scss,png,svg,json,jpg} dist",
    "dev-server": "node dist/webpack/-dev-server.mjs",
    "create-components-index": "vgis src/webapp/exports/ui.mts",
    "create-stories-index": "vgis src/webapp/exports/stories.mts"
  },
  "watch": {
    "create-stories-index": {
      "patterns": ["src/webapp/**/*.stories.tsx"],
      "extensions": ".ts,.tsx,.mts",
      "delay": 300
    },
    "create-components-index": {
      "patterns": ["src/webapp/**/*"],
      "extensions": ".ts,.tsx,.mts",
      "delay": 300
    },
    "copy-assets": {
      "patterns": ["src/**/*"],
      "extensions": ".scss,.png,.svg,.json",
      "ignore": "**/*.*ts*",
      "delay": 300
    }
  },
  "peerDependencies": {
    "@moodlenet/arangodb": "^0.9.1",
    "@moodlenet/core": "^0.9.1",
    "@moodlenet/http-server": "^0.9.1",
    "@moodlenet/organization": "^0.9.1"
  },
  "devDependencies": {
    "@moodlenet/arangodb": "^0.9.1",
    "@moodlenet/core": "^0.9.1",
    "@moodlenet/http-server": "^0.9.1",
    "@moodlenet/organization": "^0.9.1",
    "@storybook/addon-actions": "7.4.2",
    "@storybook/react": "7.4.2",
    "@types/copy-webpack-plugin": "10.1.0",
    "@types/jest": "29.5.8",
    "@types/node": "17.0.35",
    "@types/object-hash": "3.0.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7"
  },
  "dependencies": {
    "@babel/core": "7.19.0",
    "@babel/preset-env": "7.18.0",
    "@babel/preset-react": "7.17.12",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@moodlenet/component-library": "^0.9.1",
    "@moodlenet/key-value-store": "^0.9.1",
    "@mui/icons-material": "5.14.9",
    "@mui/material": "5.14.9",
    "@svgr/webpack": "6.2.1",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.5",
    "babel-plugin-direct-import": "1.0.0",
    "compression-webpack-plugin": "10.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.1",
    "file-loader": "6.2.0",
    "formik": "2.2.9",
    "html-webpack-plugin": "5.5.0",
    "less": "4.1.2",
    "less-loader": "11.0.0",
    "node-polyfill-webpack-plugin": "2.0.1",
    "object-hash": "3.0.0",
    "path-to-regexp": "6.2.1",
    "pkg-dir": "7.0.0",
    "pretty-quick": "3.1.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-refresh": "0.13.0",
    "react-refresh-typescript": "2.0.5",
