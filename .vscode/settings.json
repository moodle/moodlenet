{
  "typescript.tsdk": "node_modules/typescript/lib",
  "json.schemas": [],
  "yaml.schemas": {
    "https://asyncapi.com/schema-store/3.0.0-without-$id.json": "asyncapi/**/*.api.yml",
    "https://json-schema.org/draft-07/schema": "asyncapi/**/*.sch.yml"
    // "https://json-schema.org/draft/2020-12/schema": "asyncapi/**/*.sch.yml"
  },
  "javascript.updateImportsOnFileMove.enabled": "prompt",
  "todo-tree.filtering.excludeGlobs": [
    "**/pri/webapp-moodlenet-nextjs/zz__legacy/*/**",
    "**/pri/webapp-moodlenet-nextjs/ui-cmps/*/**",
    "**/node_modules/*/**"
  ],
  // -TODO-TREE
  // "todo-tree.tree.filterCaseSensitive": false,
  "todo-tree.highlights.useColourScheme": true,
  "todo-tree.highlights.defaultHighlight": {
    "icon": "alert",
    "type": "text",
    "foreground": "#ff4441",
    "background": "#ffffff",
    "opacity": 50,
    "iconColour": "#41c9ff"
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "uncheckbox",
      "foreground": "#000000",
      "iconColour": "#33ff00",
      "background": "#33ff00",
      "opacity": 80
    },
    "SHAREDLIB": {
      "foreground": "#000000",
      "iconColour": "#ff0095",
      "background": "#ff0095",
      "opacity": 90
    },
    "FIXME": {
      "foreground": "#000000",
      "iconColour": "#ff00ff",
      "background": "#ff00ff",
      "opacity": 90
    },
    "NOTE": {
      "foreground": "#000000",
      "iconColour": "#41c9ff",
      "background": "#41c9ff",
      "opacity": 80
    },
    "CREDITS": {
      "foreground": "#000000",
      "iconColour": "#fff241",
      "background": "#fff241",
      "opacity": 80
    }
  },
  "todo-tree.general.tags": [
    "CREDITS",
    "CREDIT",
    "THANKS",
    "THANK",
    //
    "TODO",
    //
    "FIXME",
    "FIXIT",
    "FIX",
    //
    "BUG",
    //
    "REVIEW",
    //
    "HACK",
    //
    "CHECK",
    "CHECKOUT",
    "BEWARE",
    //
    "NOTE",
    "NOTICE",
    //
    "SHAREDLIB",
    "SHARED_LIB",
    "MAKE_SHARED_LIB",
    "MAKE_SHARED_LIB",
    "NEED_SHARED_LIB",
    "SHARED_LIB_NEEDED"
  ],
  "todo-tree.general.tagGroups": {
    "FIXME": ["FIXME", "FIXIT", "FIX"],
    "NOTE": ["NOTE", "NOTICE"],
    "CHECK": ["CHECK", "CHECKOUT", "BEWARE"],
    "CREDITS": ["CREDITS", "CREDIT", "THANKS", "THANK"],
    "SHAREDLIB": [
      "SHAREDLIB",
      "SHARED_LIB",
      "MAKE_SHARED_LIB",
      "NEED_SHARED_LIB",
      "SHARED_LIB_NEEDED"
    ]
  },
  "typescript.preferences.importModuleSpecifier": "project-relative"
}
