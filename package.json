{
  "name": "moodlenet-dev",
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "description": "MoodleNet monorepo system",
  "author": "https://github.com/aleclofabbro",
  "type": "module",
  "engines": {
    "node": ">=16",
    "npm": ">=8",
    "yarn": "please-use-npm"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/core": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.0",
    "husky-init": "^8.0.0",
    "jose": "^4.13.1",
    "lerna": "^5.0.0",
    "lerna-dependency-graph": "^1.1.0",
    "lint-staged": "^13.0.3",
    "node-dev": "^7.4.3",
    "npm-watch": "^0.11.0",
    "pm2": "^5.2.2",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "storybook-addon-state": "^1.0.3",
    "typescript": "^4.7.2",
    "vscode-generate-index-standalone": "^1.6.0",
    "@moodlenet/core": "file:./packages/core",
    "@moodlenet/component-library": "file:./packages/component-library",
    "@moodlenet/arangodb": "file:./packages/arangodb",
    "@moodlenet/key-value-store": "file:./packages/key-value-store",
    "@moodlenet/crypto": "file:./packages/crypto",
    "@moodlenet/http-server": "file:./packages/http-server",
    "@moodlenet/organization": "file:./packages/organization",
    "@moodlenet/system-entities": "file:./packages/system-entities",
    "@moodlenet/email-service": "file:./packages/email-service",
    "@moodlenet/react-app": "file:./packages/react-app",
    "@moodlenet/extensions-manager": "file:./packages/extensions-manager",
    "@moodlenet/simple-email-auth": "file:./packages/simple-email-auth",
    "@moodlenet/simple-file-store": "file:./packages/simple-file-store",
    "@moodlenet/openid": "file:./packages/openid",
    "@moodlenet/ed-resource": "file:./packages/ed-resource",
    "@moodlenet/collection": "file:./packages/collection",
    "@moodlenet/web-user": "file:./packages/web-user",
    "@moodlenet/ed-meta": "file:./packages/ed-meta"
  },
  "scripts": {
    "postinstall": "npm run prj-init",
    "prj-init": "npm run prj-husky-init && npm run prj-bs && lerna run dev-init --parallel --stream && npm run prj-build",
    "prj-husky-init": "rimraf .husky && husky install && husky add .husky/pre-commit \"npm run pre-commit\"",
    "prj-bs": "lerna bootstrap",
    "dev-assistant": "lerna run dev-assistant --parallel --stream --",
    "prj-build": "tsc --build ./packages/arangodb ./packages/component-library ./packages/system-entities ./packages/core ./packages/crypto ./packages/email-service ./packages/extensions-manager ./packages/http-server ./packages/key-value-store ./packages/organization ./packages/react-app ./packages/simple-email-auth ./packages/ed-resource ./packages/collection ./packages/ed-meta ./packages/simple-file-store ./packages/openid ./packages/web-user",
    "prj-build-watch": "npm run prj-build -- --watch",
    "prj-build-clean": "npm run prj-build -- --clean",
    "dev-install-backend": "node bin/install.mjs",
    "dev-start-backend": "node bin/boot.mjs",
    "dev-start-webapp": "node bin/dev-webapp.mjs",
    "pkg-cleanup-lock": "rimraf packages/*/package-lock.json",
    "prj-cleanup-all": "npm run pkg-cleanup-all && rimraf .husky && rimraf node_modules",
    "pkg-cleanup-all": "npm run prj-cleanup-build && lerna run cleanup-all --parallel && rimraf tsconfig.tsbuildinfo && rimraf packages/*/tsconfig.tsbuildinfo && rimraf packages/*/node_modules",
    "prj-cleanup-build": "lerna run cleanup-build --parallel && rimraf packages/*/dist && rimraf packages/*/tsconfig.tsbuildinfo",
    "dep-graph": "lerna-dependency-graph -f jpg -o dependency-graph.jpg && lerna-dependency-graph -o dependency-graph.dot",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6"
  }
}
