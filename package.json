{
  "name": "moodlenet-dev",
  "version": "0.0.1",
  "license": "AGPL-3.0",
  "description": "MoodleNet monorepo system",
  "author": "https://github.com/aleclofabbro",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@material-ui/icons": "^4.11.3",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^8.0.0",
    "husky-init": "^8.0.0",
    "lerna": "^5.0.0",
    "lerna-dependency-graph": "^1.1.0",
    "lint-staged": "^13.0.3",
    "node-dev": "^7.4.3",
    "npm-watch": "^0.11.0",
    "prettier": "2.7.1",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "storybook-addon-state": "^1.0.3",
    "typescript": "^4.7.2",
    "vscode-generate-index-standalone": "^1.6.0"
  },
  "scripts": {
    "init": "husky install && npx husky add .husky/pre-commit \"npm run pre-commit\"",
    "bs": "lerna bootstrap",
    "setup": "lerna run setup --parallel --stream",
    "start-dev": "lerna run start-dev --parallel --stream -- -- ",
    "build": "lerna run build --stream",
    "clean-lib-dirs": "rimraf packages/*/lib",
    "clean-node_modules": "rimraf packages/*/node_modules",
    "clean-dirs": "yarn clean-node_modules && yarn clean-lib-dirs",
    "clean-bs": "yarn clean-dirs && yarn bs",
    "start": "lerna run start --parallel --stream",
    "test": "",
    "dep-graph": "lerna-dependency-graph -f jpg -o dependency-graph.jpg && lerna-dependency-graph -o dependency-graph.dot",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --check --ignore-path .gitignore \"**/*.+(json|ts|tsx)\"",
    "prettierjs": "prettier --check --ignore-path .gitignore \"**/*.+(js)\"",
    "dev-install-be": "node bin/install.mjs",
    "dev-backend": "node-dev bin/boot.mjs  --fork=false --trace-warnings --inspect --deps=\"-1\" --respawn --notify=false --enable-source-maps --",
    "dev-webapp": "node bin/dev-webapp.mjs",
    "dev-assistant": "lerna run dev-assistant --parallel --stream --"
  }
}
